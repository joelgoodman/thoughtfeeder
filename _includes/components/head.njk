<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <title>{{ title or site.title }}</title>
    <meta name="description" content="{{ description or site.description }}">
    {% if sitetools.env == 'development' %}
        <link rel="stylesheet" href="/assets/css/tf.css">
    {% else %}
        <link rel="stylesheet" href="/assets/css/tf.css" defer="defer">
    {% endif %}
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link href="https://fonts.googleapis.com/css2?family=Baloo+2:wght@500;700&family=Work+Sans:ital,wght@0,400;0,700;0,800;1,400&display=swap" rel="stylesheet">
    <link rel="alternate" type="application/rss+xml" href="{{ site.feed.url }}" />
    <link rel="icon" type="image/svg+xml" href="/assets/icons/favicon.svg">
    <link rel="alternate icon" href="/assets/icons/favicon.ico">
    <link rel="mask-icon" href="/assets/icons/favicon.svg" color="#6b4de0">
    <link rel="manifest" href="/site.webmanifest">
    <meta name="apple-mobile-web-app-title" content="Thought Feeder">
    <meta name="application-name" content="Thought Feeder">
    <meta name="msapplication-TileColor" content="#6b4de0">
    <meta property="og:locale" content="en_US">
    <meta property="og:type" content="website">
    <meta property="og:image:height" content="1257">
    <meta property="og:image:width" content="2400">
    <meta property="og:title" content="{{ social.fb.title or site.social.fb.title }}">
    <meta property="og:description" content="{{ social.fb.description or site.social.fb.description }}">
    <meta property="og:url" content="{{ page.url }}">
    <meta property="og:image" content="{{ site.cdn }}{{ social.fb.image or site.social.fb.image }}">
    <meta property="og:site_name" content="{{ site.title }}">
    {% if site.social.fb.app_id %}
    <meta property="fb:app_id" content="{{ site.social.fb.app_id }}">
    {% endif %}
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="{{ social.tw.title or site.social.tw.title }}">
    <meta name="twitter:description" content="{{ social.tw.description or site.social.tw.description }}">
    <meta name="twitter:site" content="{{ site.social.tw.site }}">
    <meta name="twitter:creator" content="{{ social.tw.creator or site.social.tw.creator }}">
    <meta name="twitter:image" content="{{ site.cdn }}{{ social.tw.image or site.social.tw.image }}">
</head>